{
  "name": "mini-cms",
  "version": "1.0.0",
  "description": "A mini Content Management System built with Remix and Prisma",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "prisma generate && remix build",
    "dev": "remix dev",
    "start": "remix-serve build/index.js",
    "typecheck": "tsc",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "@remix-run/node": "^2.16.7",
    "@remix-run/react": "^2.16.7",
    "@remix-run/serve": "^2.16.7",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unified": "^11.0.5",
    "zwitch": "^2.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.7",
    "@tailwindcss/typography": "^0.5.19",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vercel/remix": "^2.16.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
